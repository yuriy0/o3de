                        "IsArray": 1
                    }
                },
                {
                    "Name": "ProjectedShadowmap",
                    "ShaderInputName": "m_projectedShadowmaps",
                    "SlotType": "Input",
                    "ScopeAttachmentUsage": "Shader",
