                    "ScopeAttachmentUsage": "Shader",
                    "ImageViewDesc": {
                        "AspectFlags": [
                            "Depth"
                        ]
                    }
                },

                // Input data for lighting/shadowing
                {
                    "Name": "DirectionalLightShadowmap",
                    "ShaderInputName": "m_directionalLightShadowmap",
                    "SlotType": "Input",
                    "ScopeAttachmentUsage": "Shader",
                    "ImageViewDesc": {
                        "IsArray": 1
                    }
                },
                {
