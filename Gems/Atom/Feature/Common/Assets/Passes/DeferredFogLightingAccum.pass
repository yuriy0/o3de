{
    "Type": "JsonSerialization",
    "Version": 1,
    "ClassName": "PassAsset",
    "ClassData": {
        "PassTemplate": {
            "Name": "DeferredFogLightingAccumPassTemplate",
            "PassClass": "DeferredFogComputePass",
